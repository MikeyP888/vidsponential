{
  "buildCommand": null,
  "outputDirectory": "old_html_site/pages",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/old_html_site/pages/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}